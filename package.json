  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.21.2",
    "knex": "^0.95.4",
    "morgan": "^1.9.1",
    "pg": "^8.7.1",
    "postgrator-cli": "^3.2.0",
    "xss": "^1.0.9"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^6.2.2",
    "nodemon": "^2.0.2",
    "prettier": "2.3.2",
    "supertest": "^4.0.2"
  }
}